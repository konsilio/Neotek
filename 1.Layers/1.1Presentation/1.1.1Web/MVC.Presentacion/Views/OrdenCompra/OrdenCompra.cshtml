                                                    <label>Capasidad (Ltr) tanque tractor:</label>
                                                    <asp:TextBox ID="txtCapasidadLtrTanqueTractor" CssClass="form-control " />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-md-6">
                                                <div class="form-group">
                                                    <label>% Magnatel tractor (Ocultar)</label>
                                                    <asp:TextBox ID="txtPorcentajeMagnatelTractorOcultar" CssClass="form-control" />
                                                </div>
                                            </div>
                                        </div>
                                        <hr>
                                        <div class="row clearfix">
                                            <h6>Descarga</h6>
                                        </div>
                                        <div class="row clearfix">
                                            <div class="col-md-12">
                                                <div class="form-group">
