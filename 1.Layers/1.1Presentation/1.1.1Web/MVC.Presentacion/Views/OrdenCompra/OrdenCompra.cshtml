                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Nombre operador:</label>
                                                    <asp:TextBox ID="txtNombreOperador" CssClass="form-control" />
                                                </div>

                                            </div>
                                            <div class="col-md-4">
                                                <div class="form-group">
                                                    <label>Presión tanque tractor:</label>
                                                    <asp:TextBox ID="txtPrecsionTantqueTractor" CssClass="form-control" />
