                                        <div class="row clearfix">
                                            <div class="col-md-3 content-center">
                                                <div class="form-group">
                                                    <label>% Magnatel Almacen</label>
                                                </div>
                                            </div>
                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label>Inicial</label>
                                                    <asp:TextBox ID="txtMagnatelAlmacenInicial" CssClass="form-control" />
