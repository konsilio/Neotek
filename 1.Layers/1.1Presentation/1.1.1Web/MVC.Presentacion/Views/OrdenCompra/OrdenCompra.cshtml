                                            <div class="col-md-3">
                                                <div class="form-group">
                                                    <label>Final</label>
                                                    <asp:TextBox ID="txtMagnatelAlmacenFinal" CssClass="form-control" />
                                                </div>
                                            </div>
                                        </div>
