@using MVC.Presentacion.Models.OrdenCompra;
@model OrdenCompraModel
<section class="content home">
    <div class="container-fluid">
        <div class="block-header">
            <div class="row clearfix">
                <div class="col-lg-5 col-md-5 col-sm-12">
                    <h2>Compras</h2>
                    <ul class="breadcrumb padding-0">
                        <li class="breadcrumb-item"><a href="~/DashBoard/Vista/Dashboard.aspx"><i class="zmdi zmdi-home"></i></a></li>
                        <li class="breadcrumb-item active">
                            <label>Compras / Ordern de Compra / Nueva "</label>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="card">
                <div class="row clearfix">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label> @Model.NombreEmpresa</label>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="form-group">
                            <div class="input-group">
                                <b>Número de Requisición:</b>&nbsp;
                                <label> @Model.NumRequisicion</label>
                            </div>
                            <div class="input-group">
                                <b>Número de Orden de Compra:</b>&nbsp;
                                <label>@Model.NumOrdenCompra</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row clearfix">
                    <div class="col-lg-3 col-md-12">
                        <div class="form-group form-float">
                            <b>Fecha requerida:</b>
                            <dtp:DateTimePicker ID="txtFechaRequerida" CssClass="dataTxt" Enabled="false" />
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-12">
                        <div class="form-group form-float">
                            <b>Solicitante:</b>
                            <input name="Solicitante" value="@Model.Solicitante" class="form-control disabled" />
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-12 form-float">
                        <div class="form-group">
                            <b>Motivo de compra:</b>
                            <textarea name="MotivoCompra" class="form-control  no-resize disabled" placeholder="Error de carga" rows="4">@Model.MotivoCompra</textarea>
                        </div>
                    </div>
                    <div class="col-lg-3 col-md-12">
                        <div class="form-group form-float">
                            <%--Se requiere en--%>
                            <b>Se requiere en:</b>
                            <textarea name="RequeridoEn" class="form-control  no-resize disabled" placeholder="Error de carga" rows="4">@Model.RequeridoEn</textarea>
                        </div>
                    </div>
                </div>
                @if (ViewBag.NombrePorvedor != null)
                {
                    <div class="row clearfix">
                        <div class="col-sm-6">
                            <div class="form-group">
                                <div class="input-group">
                                    <b>Proveedor:</b>&nbsp;
                                    <label>@ViewBag.NombrePorvedor</label>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <dtp:DateTimePicker ID="dtpFechaEntrada" />
                        </div>
                    </div>
                }
                @if (ViewBag.MensajeError != null)
                {
                    <div class="row clearfix">
                        <div id="divMensajeError" class="container">
                            <div class="alert alert-danger">
                                <div class="alert-icon">
                                    <i class="zmdi zmdi-block"></i>
                                </div>
                                <strong>
                                    <label>@ViewBag.MensajeError</label>
                                </strong>
                            </div>
                        </div>
                    </div>
                }
                @if (Model.IdOrdenCompraEstatus.Equals(0))
                {
                    <div class="body">
                        <div class="row clearfix" id="divCrearOC">
                            <div class="table-responsive">
                                <table CssClass="table table-hover">
                                    <thead>
                                        <tr>
                                            <td><b>Tipo</b></td>
                                            <td><b>Producto</b></td>
                                            <td><b>Cantidad</b></td>
                                            <td><b>Unidad</b></td>
                                            <td><b>Aplicación</b></td>
                                            <td><b>Centro de costo</b></td>
                                            <td><b>Proveedor</b></td>
                                            <td><b>Cuenta Contable</b></td>
                                            <td><b>Precio</b></td>
                                            <td><b>%Desc</b></td>
                                            <td><b>%IVA</b></td>
                                            <td><b>%IEPS</b></td>
                                            <td><b>Importe</b></td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var _pord in Model.OrdenCompraProductos)
                                        {
                                            <tr>
                                                <td>@_pord.TipoProducto</td>
                                                <td>@_pord.Producto</td>
                                                <td>@_pord.Cantidad</td>
                                                <td>@_pord.Unidad</td>
                                                <td>@_pord.Aplicacion</td>
                                                <td>@_pord.CentroCosto</td>
                                                <td>
                                                    <select class="form-control z-index">
                                                        @foreach (var prov in ViewBag.Proveedores)
                                                        {
                                                            <option value="@prov.IdProveedor">@prov.Descripcion</option>
                                                        }
                                                    </select>
                                                </td>
                                                <td>
                                                    <select class="form-control z-index">
                                                        @foreach (var cc in ViewBag.CuentasContables)
                                                        {
                                                            <option value="@cc.IdCuentaContable">@cc.Descripcion</option>
                                                        }
                                                    </select>
                                                </td>
                                                <td><input type="text" id="Precio" class="form-control" /></td>
                                                <td><input type="text" id="Descuento" class="form-control" /></td>
                                                <td>
                                                    <select class="form-control z-index">
                                                        @foreach (var prov in ViewBag.IVAs)
                                                        {
                                                            <option value="@prov.IdIva">@prov.Descripcion</option>
                                                        }
                                                    </select>
                                                </td>
                                                <td>
                                                    <select class="form-control z-index">
                                                        @foreach (var prov in ViewBag.IEPSs)
                                                        {
                                                            <option value="@prov.IdIeps">@prov.Descripcion</option>
                                                        }
                                                    </select>
                                                </td>
                                                <td><label id="Importe" /></td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="row clearfix" id="divAutorizarOC">
                            <div class="body table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <td><b>Tipo</b></td>
                                            <td><b>Producto</b></td>
                                            <td><b>Requeridos</b></td>
                                            <td><b>Unidad</b></td>
                                            <td><b>Detalle</b></td>
                                            <td><b>Centro de costo</b></td>
                                            <td><b>Cuenta Contable</b></td>
                                            <td><b>Cantidad</b></td>
                                            <td><b>Precio</b></td>
                                            <td><b>%Desc</b></td>
                                            <td><b>%IVA</b></td>
                                            <td><b>%IEPS</b></td>
                                            <td><b>Importe</b></td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var prod in Model.OrdenCompraProductos)
                                        {
                                            <tr>
                                                <td>@prod.TipoProducto</td>
                                                <td>@prod.Producto</td>
                                                <td>@prod.Cantidad</td>
                                                <td>@prod.Unidad</td>
                                                <td>@prod.Aplicacion</td>
                                                <td>@prod.CentroCosto</td>
                                                <td>@prod.IdCuentaContable</td>
                                                <td>@prod.CantidadAComprar</td>
                                                <td>@prod.Precio</td>
                                                <td>@prod.Descuento</td>
                                                <td>@prod.IVA</td>
                                                <td>@prod.IEPS</td>
                                                <td>@prod.Importe</td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                        <div class="row clearfix" id="divEntrada">
                            <div class="body table-responsive">
                                <table class="table table-hover">
                                    <thead>
                                        <tr>
                                            <td><b>Tipo</b></td>
                                            <td><b>Producto</b></td>
                                            <td><b>Requeridos</b></td>
                                            <td><b>Unidad</b></td>
                                            <td><b>Aplicación</b></td>
                                            <td><b>Cantidad</b></td>
                                            <td hidden><b>Justificación</b></td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var prod in Model.OrdenCompraProductos)
                                        {
                                            <tr>
                                                <td>@prod.TipoProducto</td>
                                                <td>@prod.Producto</td>
                                                <td>@prod.Cantidad</td>
                                                <td>@prod.Unidad</td>
                                                <td>@prod.Aplicacion</td>
                                                <td>@prod.Importe</td>
                                                <td hidden><input id="Cantidad" type="text" class="form-control" /></td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                }
            </div>
            @if (ViewBag.Complementogas != null)
            {

                <div class="card" id="divGvvComplementoGas">
                    <div class="body">
                        <div class="row clearfix">
                            <div class="row clearfix">
                                <div class="table-responsive">
                                    <asp:GridView CssClass="table table-hover" OnRowEditing="dgListaproductos_RowEditing" ID="gvComplementoGas" Width="100%" AutoGenerateColumns="false" OnRowDataBound="dgListaproductos_RowDataBound">
                                        <Columns>
                                            <asp:TemplateField>
                                                <HeaderTemplate>
                                                    <b>Tipo</b>
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:Label ID="lbldgTipo" Text='<%# Bind("TipoProducto") %>' />
                                                </ItemTemplate>
                                                <ItemStyle CssClass="text-sm-center" />
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    <b>Producto</b>
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:Label ID="lbldgProducto" Text='<%# Bind("Producto") %>' />
                                                    <asp:Label ID="lblidProducto" Text='<%# Bind("IdProducto") %>' />
                                                </ItemTemplate>
                                                <ItemStyle CssClass="text-sm-center" />
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    <b>Cantidad</b>
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:Label ID="lbldgCantidad" Text='<%# Bind("CantidadAComprar") %>' />
                                                </ItemTemplate>
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    <b>Unidad</b>
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:Label ID="lbldgUnidad" Text='<%# Bind("Unidad") %>' />
                                                </ItemTemplate>
                                                <ItemStyle CssClass="text-sm-center" />
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    <b>Aplicación</b>
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:Label ID="lbldgAplicacion" Text='<%# Bind("Aplicacion") %>' />
                                                </ItemTemplate>
                                                <ItemStyle CssClass="text-sm-center" />
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    <b>Centro de costo</b>
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:Label ID="lbldgCentroCosto" Text='<%# Bind("CentroCosto") %>' />
                                                    <asp:Label ID="lbldgidCentroCosto" Text='<%# Bind("IdCentroCosto") %>' />
                                                </ItemTemplate>
                                                <ItemStyle CssClass="text-sm-center" />
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    Proveedor
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:DropDownList ID="ddlProveedor"></asp:DropDownList>
                                                </ItemTemplate>
                                            </asp:TemplateField>
                                            <asp:TemplateField>

                                                <HeaderTemplate>
                                                    Cuenta Contable
                                                </HeaderTemplate>
                                                <ItemTemplate>
                                                    <asp:DropDownList ID="ddlCuentaContable"></asp:DropDownList>
                                                </ItemTemplate>
                                            </asp:TemplateField>
                                        </Columns>
                                    </asp:GridView>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="card" id="divDatosComplementoGas">
                    <div class="body">
                        <div class="panel-group" id="accordion_1" role="tablist" aria-multiselectable="true">
