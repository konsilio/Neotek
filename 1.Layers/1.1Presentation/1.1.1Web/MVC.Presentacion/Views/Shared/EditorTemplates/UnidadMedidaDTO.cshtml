@model MVC.Presentacion.Models.Catalogos.UnidadMedidaDTO
<tr>
    <td>@Model.Nombre </td>
    <td>@Model.Acronimo </td>
    <td>@Model.Descripcion </td>    
    <td>
        <a href="@Url.Action("EditarUnidadMedida", "Producto", new { Id = Model.IdUnidadMedida })">
            <i class="material-icons">edit</i>
            <span class="icon-name"></span>
        </a>
        <a href="@Url.Action("EliminarUnidadMedida", "Producto", new { Id = Model.IdUnidadMedida })">
            <i class="material-icons">delete</i>
            <span class="icon-name"></span>
        </a>
    </td>
</tr>

