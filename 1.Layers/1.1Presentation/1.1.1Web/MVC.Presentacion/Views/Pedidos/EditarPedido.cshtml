<head>
    <script src="~/Resources/js/lib/jquery-1.9.1.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            //    $("#btnRegresaRevision").click(function () {
            //        var form = $(this).parent("form");
            //        form.attr('action', '<%= Url.RouteUrl(new { Controller = "Pedidos", Action = "Index"}) %>');
            //        form.attr('method', 'post');
            //    });

            $("#btnSave").click(function () {

                var calificacion = [];
                var selected1 = $("input[name='estrellas1']:checked").val();
                var selected2 = $("input[name='estrellas2']:checked").val();
                var selected3 = $("input[name='estrellas3']:checked").val();
                var selected4 = $("input[name='estrellas4']:checked").val();
                var selected5 = $("input[name='estrellas5']:checked").val();
                if (selected1 != undefined)
                    calificacion.push({IdPedido:@Model.IdPedido, IdPregunta:1,Respuesta: selected1 });
                if (selected2 != undefined)
                    calificacion.push({IdPedido:@Model.IdPedido, IdPregunta:2,Respuesta: selected2 });
                if (selected3 != undefined)
                    calificacion.push({IdPedido:@Model.IdPedido, IdPregunta:3,Respuesta: selected3 });
                if (selected4 != undefined)
                    calificacion.push({ IdPedido:@Model.IdPedido,IdPregunta:4,Respuesta: selected4 });
                if (selected5 != undefined)
                    calificacion.push({IdPedido:@Model.IdPedido, IdPregunta:5,Respuesta: selected5 });

                calificacion = JSON.stringify({ 'calificacion': calificacion });
                
                $.ajax({
                    contentType: 'application/json; charset=utf-8',
                    dataType: 'json',
                    type: 'POST',
                    url: '/Pedidos/GuardarEncuesta',
                    data: calificacion,
                    success: function () {
                       
                    },
                    failure: function (response) {
                        
                    }
                });

            });
        });

    </script>
    <style type="text/css">
        input[type = "radio"] {
            display: none;
        }

        .radio {
            display: none;
        }

        label {
            color: grey;
            font-size: 20px;
        }

        .clasificacion {
            direction: rtl; /* right to left */
            unicode-bidi: bidi-override; /* bidi de bidireccional */
        }

        label:hover,
        label:hover ~ label {
            color: orange;
        }

        input[type = "radio"]:checked ~ label {
            color: orange;
        }

        .radio:checked {
            color: orange;
        }

        .alinear {
            text-align: center;
        }
    </style>
</head>
