@model MVC.Presentacion.Models.Catalogos.CentroCostoModel
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Centro Costo</title>
    <link rel="stylesheet" href="~/assets/plugins/bootstrap-select/css/bootstrap-select.css" />
</head>
<body style="padding-top:10px">
    @using (Html.BeginForm("Crear", "CentroCosto", FormMethod.Post))
    {
        <section class="content home">
            <div class="container-fluid">
                <div class="block-header">
                    <div class="row clearfix">
                        <div class="col-lg-5 col-md-5 col-sm-12">
                            <ul class="breadcrumb padding-0">
                                <li class="breadcrumb-item"><a href="~/DashBoard/Vista/Dashboard.aspx"><i class="zmdi zmdi-home"></i></a></li>
                                <li class="breadcrumb-item active">
                                    <Label> Catalogos / Centros de costo</Label>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
                <div class="row clearfix">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="card">
                            <div class="header">
                            </div>
                            <div class="body">
                                <div class="row clearfix">
                                    <div class="col-md-3 form-control-label">
                                        <label>Numero</label>
                                        <div class="form-group">
                                            @Html.TextBoxFor(m => m.Numero, new { @class = "form-control", @placeholder = "Numero de centro de costo" })
                                            <Label class="alert-danger" hidden> Campo requerido</Label>
                                        </div>
                                    </div>
                                    <div class="col-md-6 form-control-label">
                                        <label for="txtDescripcion">Descripción</label>
                                        <div class="form-group">
                                            @Html.TextBoxFor(m => m.Descripcion, new { @class = "form-control", placeholder = "Descripción", @style = "max-width:initial" })
                                            <Label class="alert-danger" hidden>Text="Campo requerido" </Label>
                                        </div>
                                    </div>
                                    <div class="col-md-3 form-control-label">
                                        <label for="ddlTipoCentroCosto">Tipo centro de costo</label>
                                        <div class="form-group">
                                            <select name=IdTipoCentroCosto class="form-control z-index show-tick" data-live-search="true">
                                                <option id="0">Seleccione uno</option>
                                                @foreach (var tp in ViewBag.TiposCentrosCosto)
                                                {
                                                    <option id="@tp.IdTipoCentroCosto">@tp.Descripcion</option>
                                                }
                                            </select>
                                        </div>
                                        <Label ID="reqTipoCC" class="alert-danger" hidden Text="Campo requerido" />
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    <div class="col-md-3 form-control-label">
                                        <label for="ddlEquipoTransporte">Equipo Transporte</label>
                                        <div class="form-group">
                                            <select id="IdEquipoTransporte" class="form-control z-index show-tick" data-live-search="true">
                                                <option id="0">Seleccione uno</option>
                                                @foreach (var tp in ViewBag.EquipoTransporte)
                                                {
                                                    <option id="@tp.IdEquipoTransporte">@tp.Descripcion</option>
                                                }
                                            </select>
                                        </div>
                                        <Label id="reqEquipoTrasnporte" class="alert-danger" hidden="hidden">Campo requerido </Label>
                                    </div>
                                    @*<div class="col-md-3 form-control-label">
                                        <label for="ddlVehiculo">Vehiculo</label>
                                        <div class="form-group">
                                            <select id="IdVehiculo" class="form-control z-index show-tick" disabled data-live-search="true">
                                                <option id="0">Seleccione uno</option>
                                            </select>
                                        </div>*@
                                    <Label ID="reqVehiculo" class="alert-danger" hidden="hidden">Campo requerido</Label>

                                    <div class="col-md-3 form-control-label">
                                        <label for="ddlUnidadAlmacenGas">Unidad de almacen de gas</label>
                                        <div class="form-group">
                                            <select id="IdVehiculo" class="form-control z-index show-tick" data-live-search="true">
                                                <option id="0">Seleccione uno</option>
                                                @foreach (var tp in ViewBag.UnidadAlmacenGas)
                                                {
                                                    <option id="@tp.IdCAlmacenGas">@tp.Numero</option>
                                                }
                                            </select>
                                        </div>
                                        <Label ID="reqUnidad" class="alert-danger" hidden="hidden">Campo requerido</Label>
                                    </div>
                                    <div class="col-md-3 form-control-label">
                                        <label for="ddlEstacionCarburacion">Estacion de carburación</label>
                                        <div class="form-group">
                                            <select id="IdEstacionCarburacion" class="form-control z-index show-tick" data-live-search="true">
                                                <option id="0">Seleccione uno</option>
                                                @foreach (var tp in ViewBag.EstacionesCarburacion)
                                                {
                                                    <option id="@tp.IdEstacionCarburacion">@tp.Nombre</option>
                                                }
                                            </select>
                                        </div>
                                        <Label ID="reqEstacion" class="alert-danger" hidden="hidden">Campo requerido</Label>
                                    </div>
                                    <div class="col-md-3 form-control-label">
                                        <b>&nbsp; </b>
                                        @if (ViewBag.EsEdicion == null)
                                        {
                                            <button btn id="BtnCrear" formaction="Crear" class="btn btn-primary btn-icon  btn-icon-mini btn-round">
                                                <i class="zmdi zmdi-plus"></i>
                                            </button>
                                        }
                                        else
                                        {
                                            <button btn id="btnUpDate" formaction="Modificar" class="btn btn-primary btn-icon  btn-icon-mini btn-round">
                                                <i class="zmdi zmdi-swap"></i>
                                            </button>
                                        }
                                    </div>
                                </div>
                                <div class="row clearfix">
                                    @*<div class="col-md-3 form-control-label">
                                                <label for="ddlCamioneta">Camioneta</label>
                                                <div class="form-group">
                                                    <select id="IdCamioneta" class="form-control z-index show-tick" disabled data-live-search="true">
                                                        <option id="0">Seleccione uno</option>
                                                    </select>
                                                </div>
                                                <Label ID="reqCamnioneta" class="alert-danger" hidden="hidden">Campo requerido</Label>
                                            </div>
                                        <div class="col-md-3 form-control-label">
                                                <label for="ddlPipa">Pipa</label>
                                                <div class="form-group">
                                                    <select id="IdPipa" class="form-control z-index show-tick" disabled data-live-search="true">
                                                        <option id="0">Seleccione uno</option>

                                                    </select>
                                                </div>
                                                <Label ID="reqPipa" class="alert-danger" hidden="hidden">Campo requerido</Label>
                                            </div>
                                        <div class="col-md-3 form-control-label">
                                                <label for="ddlCilindroGas">Cilindro de Gas</label>
                                                <div class="form-group">
                                                    <select id="IdCilindroGas" class="form-control z-index show-tick" disabled data-live-search="true">
                                                        <option id="0">Seleccione uno</option>
                                                    </select>
                                                </div>
                                                <Label ID="reqCilindro" CssClass="alert-danger" hidden="hidden">Campo requerido</Label>
                                            </div>*@
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row clearfix">
                    <div class="col-lg-12 col-md-12 col-sm-12">
                        <div class="card">
                            <div class="header">
                            </div>
                            <div class="body">
                                <div class="row clearfix">
                                    <div class="col-md-4 form-control-label">
                                        <label for="ddlFiltroGasera">Gasera</label>
                                        <div class="form-group">
                                            <select name="idEmpresa" class="form-control z-index show-tick" data-live-search="true">
                                                <option value="0">Seleccione uno</option>
                                                @foreach (var item in ViewBag.Empresas)
                                                {

                                                }
                                            </select>
                                        </div>
                                    </div>
                                    <div class="col-md-4 form-control-label">
                                        <label for="txtFiltroNumero">Numero</label>
                                        <div class="form-group">
                                            <input type="text" id="txtFiltroNumero" class="form-control" />
                                        </div>
                                    </div>
                                    <div class="col-md-4 form-control-label">
                                        <label for="ddlFiltroCentroCosto">Tipo centro de costo</label>
                                        <div class="form-group">
                                            <select></select>
                                        </div>
                                    </div>
                                </div>
                                @if (ViewBag.MensajeError != null)
                                {
                                    <div id="DivAlerta" class="container">
                                        <div class="alert alert-danger">
                                            <div class="alert-icon">
                                                <i class="zmdi zmdi-block"></i>
                                            </div>
                                            <strong>
                                                @ViewBag.MensajeError
                                            </strong>
                                        </div>
                                    </div>
                                }
                                <div class="row clearfix">
                                    <div class="table-responsive">
                                        <table class="table table-bordered">
                                            <thead>
                                                <tr>
                                                    <td><b>Gasera</b></td>
                                                    <td><b>Número</b></td>
                                                    <td><b>Descripción</b></td>
                                                    <td><b>Tipo</b></td>
                                                    <td><b>Acción</b></td>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                @Html.EditorFor(m => m.CentrosCostos)
                                            </tbody>
                                        </table>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </section>
        <div class="modal fade" id="ModalConfirmacion" tabindex="-1" role="dialog">
            <div class="modal-dialog modal-sm" role="document">
                <div class="modal-content">
                    <div class="modal-header">
                        <h4 class="title" id="ModalConfirmacionLabel">¿Esta seguro?</h4>
                    </div>
                    <div class="modal-footer">
                        <input type="submit ID="BtnCrear" CssClass="btn btn-raised btn-primary btn-round" Text="Si" OnClick="BtnCrear_Click" />
                        <button type="button" class="btn btn-danger btn-simple btn-round waves-effect" data-dismiss="modal">Cancelar</button>
                    </div>
                </div>
            </div>
        </div>
    }
    </body>
</html>
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.1/jquery.min.js"></script>
<script src="http://harvesthq.github.io/chosen/chosen.jquery.js"></script>
    